ANCHOR_PROVIDER_URL=https://api.devnet.solana.com
ANCHOR_WALLET=/Users/me/.config/solana/id.json
LUNARCRUSH_API_KEY=your_lunarcrush_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
STRATEGY=1

# Position size configuration
POSITION_SIZE_TYPE=constant  # Options: constant, range, max_balance
POSITION_SIZE=100           # Used when type is 'constant'
MIN_POSITION_SIZE=50        # Used when type is 'range'
MAX_POSITION_SIZE=200       # Used when type is 'range'

MAX_OPEN_ATTEMPTS=3
MAX_OPEN_POSITIONS=10
MIN_SECONDS_BETWEEN_RUNS=60
MAX_SECONDS_BETWEEN_RUNS=600

# Funding rate configuration
MIN_YIELD=5
MAX_YIELD=999999